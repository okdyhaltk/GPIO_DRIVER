# Set Environment on Nvidia Xavier NX
>
	export BUILD_ROOT_PATH=/home/okd-build/WORK_AREA1/NVIDIA/EAC-2000
	export CROSS_COMPILE_AARCH64_PATH=$BUILD_ROOT_PATH/4.toolchain
	export CROSS_COMPILE_AARCH64=$BUILD_ROOT_PATH/4.toolchain/bin/aarch64-buildroot-linux-gnu-
	export BRANCH_NAME=v1.3
	export TEGRA_KERNEL_OUT=$BUILD_ROOT_PATH/3.kernel_builded/$BRANCH_NAME
            
	export KERNEL_MODULES_OUT=$TEGRA_KERNEL_OUT/TMP
	export LOCALVERSION=-tegra            

# Make
>
	make ARCH=arm64 O=$TEGRA_KERNEL_OUT CROSS_COMPILE=${CROSS_COMPILE_AARCH64} tegra_defconfig
	make ARCH=arm64 O=$TEGRA_KERNEL_OUT CROSS_COMPILE=${CROSS_COMPILE_AARCH64} menuconfig
	make ARCH=arm64 O=$TEGRA_KERNEL_OUT CROSS_COMPILE=${CROSS_COMPILE_AARCH64} -j8 Image
	make ARCH=arm64 O=$TEGRA_KERNEL_OUT CROSS_COMPILE=${CROSS_COMPILE_AARCH64} -j8 modules
	make ARCH=arm64 O=$TEGRA_KERNEL_OUT CROSS_COMPILE=${CROSS_COMPILE_AARCH64} -j8 dtbs
	make ARCH=arm64 O=$TEGRA_KERNEL_OUT INSTALL_MOD_PATH=$KERNEL_MODULES_OUT modules_install

	cd ../../extra/GPIO/
	export KERNEL_PATH=$TEGRA_KERNEL_OUT
	make clean
	make ARCH=arm64 O=$TEGRA_KERNEL_OUT CROSS_COMPILE=${CROSS_COMPILE_AARCH64}
	make ARCH=arm64 -C $TEGRA_KERNEL_OUT M=$PWD INSTALL_MOD_PATH=$KERNEL_MODULES_OUT modules_install
